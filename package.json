{
    "name": "chuck-norris-jokes",
    "version": "1.0.0",
    "description": "This is an app created with react that connects to the Chuck Norris Jokes API",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --mode development --open --port 3000",
        "build": "webpack",
        "lint": "eslint --fix '**/*.js' --ignore-pattern node_modules/",
        "format": "prettier --write .",
        "prepare": "husky install"
    },
    "lint-staged": {
        "src/*.js": [
            "npm run format",
            "npm run lint"
        ]
    },
    "keywords": [
        "react"
    ],
    "author": "David Victoria <davidvictoria64@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.16.5",
        "@babel/preset-env": "^7.16.5",
        "@babel/preset-react": "^7.16.5",
        "babel-loader": "^8.2.3",
        "css-loader": "^6.5.1",
        "eslint": "^8.5.0",
        "eslint-config-airbnb": "^19.0.2",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "lint-staged": "^12.1.3",
        "prettier": "^2.5.1",
        "style-loader": "^3.3.1",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.6.0",
        "husky": "^7.0.0"
    }
}
